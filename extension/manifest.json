{
  "manifest_version": 2,
  "name": "Japanese Dictionary Popup",
  "version": "1.0",
  "description": "Look up Japanese words with dictpopup",
  "permissions": [
    "nativeMessaging"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
